                                            <fo:block>
                                                <fo:table-and-caption border-collapse="separate" width="600px" space-before="1em" space-after="10px" page-break-inside="avoid"><fo:table-caption start-indent="0" end-indent="0" keep-with-next="always"><fo:block-container font-weight="bold" space-before="0.5em" page-break-after="avoid" text-align="center"><fo:block end-indent="0" start-indent="0"/></fo:block-container></fo:table-caption><fo:table relative-align="baseline" chfox:html-table-width-policy="yes" table-layout="auto" width="100%" background-color="rgba(225 , 245 , 255 , 1)" border-bottom-color="black" fox:border-after-start-radius="10px" fox:border-after-end-radius="10px" border-bottom-width="2pt" border-bottom-style="none" border-left-color="black" border-left-width="2pt" border-left-style="none" border-right-color="black" border-right-width="2pt" border-right-style="none" border-top-color="black" fox:border-before-start-radius="10px" fox:border-before-end-radius="10px" border-top-width="2pt" border-top-style="none" hyphenate="false" max-width="600px" padding-bottom="12px" padding-left="12px" margin-left="0" padding-right="12px" margin-right="0" padding-top="12px" space-before="0" space-after="0"><fo:table-column column-width="6.6666665%"/><fo:table-column column-width="10%"/><fo:table-column column-width="83.333336%"/><fo:table-body display-align="center">
